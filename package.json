{
  "name": "gkd-subscription",
  "type": "module",
  "version": "1.15.0",
  "main": "./dist/gkd.json5",
  "unpkg": "./dist/gkd.json5",
  "jsdelivr": "./dist/gkd.json5",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --cache --write .",
    "lint": "eslint --cache --fix ./**/*.{js,ts,jsx,tsx}",
    "build": "tsc --noEmit && tsx ./scripts/build.ts",
    "check": "tsc --noEmit && tsx ./scripts/check.ts",
    "updateSubs": "tsx ./scripts/updateSubs.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/types": "^19.0.3",
    "@gkd-kit/selector": "0.0.22",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "dayjs": "^1.11.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-define-config": "2.1.0",
    "husky": "^9.0.11",
    "json5": "^2.2.3",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "picocolors": "^1.0.0",
    "pinyin-pro": "3.19.6",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "author": "梦念逍遥",
  "license": "MIT",
  "homepage": "https://github.com/MengNianxiaoyao/gkd-subscription#readme",
  "bugs": {
    "url": "https://github.com/MengNianxiaoyao/gkd-subscription/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MengNianxiaoyao/gkd-subscription.git"
  },
  "volta": {
    "node": "20.12.0"
  },
  "packageManager": "pnpm@8.15.5",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --cache --write --ignore-unknown"
    ],
    "*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@gkd-kit/api": "^0.0.5"
  }
}
